def bin(ls,target):
    start=0
    end=len(ls)-1

    while start<=(len(ls)-1):
        mid = (start + end) // 2
        if target==ls[mid]:
            return mid
        elif target>ls[mid]:
            start=mid+1
        elif target<ls[mid]:
            end=mid-1
    return -1
print(bin([1,4,5,8,9],9))
