1.def foo(n):
    if n<0:
        return
    print(n)
    foo(n-1)
foo(3)

2.def foo(n):
    if n<0:
        return
    foo(n - 1)
    print(n)
foo(3)
3.def foo(ls):
    if len(ls)==0:
        return
    print(ls[0])
    foo(ls[1:])
foo([0,8,9])

4.def foo(x):
    if x<10:
        return x
    return x%10 +foo(x//10)
print(foo(123))


5.def foo(x,y):
    if y==0:
        return x
    return foo(y,x%y)
print(foo(0,90))

