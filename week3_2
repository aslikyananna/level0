1.set1={1,2,3}
set2={1,2}
miavorum=set1|set2
hatum=set1&set2
simetrik=set1^set2


2.1.set1={1,2,3}
set2={1,2}

simetrik=set1^set2

3.def foo(set1,set2):
    if set1.issubset(set2):
        return True
    elif set2.issubset(set1):
        return True
    else:
        return False
print(foo(set1,set2))


4.
set1={1,2,3}
for i in set1.copy():
    if i%2!=0:
        set1.remove(i)
        set1.add(i+101)
print(set1)
